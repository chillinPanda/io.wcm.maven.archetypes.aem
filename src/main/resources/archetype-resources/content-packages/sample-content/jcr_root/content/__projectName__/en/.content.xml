#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="#if($optionEditableTemplates=="y")/conf/${projectName}/settings/wcm/templates/homepage#{else}/apps/${projectName}#if($optionMultiBundleLayout=='y')/core#end/templates/content/homepage#end"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        sling:resourceType="#if($optionEditableTemplates=="y")${projectName}#if($optionMultiBundleLayout=='y')/core#end/components/global/page#{else}${projectName}#if($optionMultiBundleLayout=='y')/core#end/components/content/page/homepage#end"
        navTitle="HOME"
        pageTitle="${projectName} Sample Site">
#if ($optionEditableTemplates == "y")
        <root jcr:primaryType="nt:unstructured" sling:resourceType="wcm/foundation/components/responsivegrid">
#end
        <stage
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${projectName}#if($optionMultiBundleLayout=='y')/core#end/components/content/stage/stageheader"
#if ($optionWcmioHandler=='y')
            linkMediaDownload="{Boolean}false"
            linkType="internal"
            linkWindowFeatures="default"
            linkWindowTarget="_self"
            mediaRef="/content/dam/${projectName}/sample-image.jpg"
            subtitle="Sample site generated by wcm.io AEM Archetype"
            title="${projectName} Sample Site"
#else
            fileReference="/content/dam/${projectName}/sample-image.jpg"
#end>
        </stage>
        <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="#if($optionEditableTemplates == "y")wcm/foundation/components/responsivegrid#{else}#if($optionWcmioHandler=='y')wcm-io/wcm/parsys/components/parsys#{else}foundation/components/parsys#end#end">
            <contentrichtext
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${projectName}#if($optionMultiBundleLayout=='y')/core#end/components/content/common/contentRichText"
#if ($optionWcmioHandler=='n')
                textIsRich="true"
#end
                text="&lt;p&gt;Lorem ipsum dolor sit amet, consectetur adipiscing elit. Cras arcu mi, commodo ut interdum quis, aliquet sed sapien. In hac habitasse platea dictumst. Pellentesque habitant morbi tristique senectus et netus et malesuada fames ac turpis egestas. Integer sit amet venenatis velit, sit amet pulvinar dolor. Donec in risus enim. Nullam ultricies dui posuere blandit euismod. Aenean varius porta aliquet. Mauris sed tempor massa, malesuada rhoncus tortor. Vestibulum vitae viverra enim. Duis feugiat mollis tortor, a efficitur massa fringilla ut.&lt;/p&gt;"/>
        </content>
#if ($optionEditableTemplates == "y")
        </root>
#end
    </jcr:content>
#if ( $optionContextAwareConfig == "y" )
    <tools/>
#end
</jcr:root>
