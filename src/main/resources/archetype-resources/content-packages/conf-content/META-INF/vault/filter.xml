<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/conf/${projectName}">
        <include pattern="/conf/${projectName}"/>
        <include pattern="/conf/${projectName}/rep:policy(/.*)?"/>
    </filter>
#if( $optionEditableTemplates == "y" )
    <filter root="/conf/${projectName}/settings/wcm">
        <exclude pattern="^.*/dam:thumbnails(/.*)?$"/>
    </filter>
#end
</workspaceFilter>
