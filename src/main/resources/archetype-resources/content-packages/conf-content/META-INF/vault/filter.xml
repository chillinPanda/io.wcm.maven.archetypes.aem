<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/conf/${projectName}">
        <include pattern="/conf/${projectName}/rep:policy"/>
        <exclude pattern="/conf/${projectName}/*"/>
    </filter>
#if( $optionEditableTemplates == "y" && $optionAemVersion != "6.1" && $optionAemVersion != "6.2" )
    <filter root="/conf/${projectName}/settings/wcm"/>
#end
</workspaceFilter>
