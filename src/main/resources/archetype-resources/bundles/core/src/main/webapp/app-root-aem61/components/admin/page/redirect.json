{
  "jcr:primaryType": "cq:Component",
  "sling:resourceSuperType": "wcm-io/handler/link/components/page/redirect",

  "cq:dialog": {
    "jcr:primaryType": "nt:unstructured",
    "jcr:title": "Page",
    "extraClientlibs": [
      "cq.common.wcm",
      "cq.siteadmin.admin.properties"
    ],
    "mode": "edit",
    "sling:resourceType": "cq/gui/components/authoring/dialog",
    "content": {
      "class": "cq-dialog-content-page",
      "sling:resourceType": "granite/ui/components/foundation/container",
      "items": {
        "tabs": {
          "rel": "cq-siteadmin-admin-properties-tabs",
          "sling:resourceType": "granite/ui/components/foundation/container",
          "layout": {
            "type": "nav",
            "sling:resourceType": "granite/ui/components/foundation/layouts/tabs"
          },
          "items": {

            "link": {
              "sling:resourceType": "granite/ui/components/foundation/include",
              "jcr:title": "Redirect Link",
              "path": "${projectName}#if($optionMultiBundleLayout=='y')/core#end/components/global/include/linkRefNoTitleTab"
            },

            "redirectStatus": {
              "sling:resourceType": "granite/ui/components/foundation/include",
              "jcr:title": "Redirect Status",
              "path": "wcm-io/handler/link/components/global/include/redirectStatus"
            }

          }
        }
      }
    }
  }
  
}
